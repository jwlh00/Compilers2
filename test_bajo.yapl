class Circle {

    radius : int;
    color : string;
    ant : string <- "Hola";
    

    sum(g : int, h : int, k : int) : int{
         x : int;
         z : bool <- false;
         x <- g + h + k + 1;

        if x < 0 then
            out_string("less than 0");
        fi;

        while z = true loop
            out_string("less than 0");
        pool;

         return x;
    };

    init() : void {
        radius  <- 100;
        color <- "Red";

        return void;
    };


};

class main {

	main() : void {
		circulo : Circle;
		circulo.init();
        a : int <- 1;
        b : int <- 2;
        c : int <- 3;
        circulo.sum(a,b,c);

        return void;
	};

};