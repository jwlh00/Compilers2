class AverageCalculator {

    getAvg(a : int, b : int, c : int) : int {
        total : int;
        avg : int;
        total <- a + b + c;
        avg <- total / 3;
        return avg;
    };
};

class main {

    main() : void {
        result : int;
        myCalc : AverageCalculator;
        myCalc.getAvg(10, 2, 5);
        out_string("The avg is: ");
        out_int(result);
        return void;
    };
};
